<section class="bg-white">
<div class="container">
  <h1 class="gotham-bold">Mobile Apps</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Ios link</th>
        <th>Android link</th>
        <th>Ios sensortower link</th>
        <th>Android sensortower link</th>
        <th>Company</th>
        <th colspan="2"></th>
      </tr>
    </thead>

    <tbody>
      <% @mobile_apps.each do |mobile_app| %>
        <tr>
          <td><%= link_to mobile_app.name, mobile_app %></td>
          <td><%= mobile_app.ios_link %></td>
          <td><%= mobile_app.android_link %></td>
          <td><%= mobile_app.ios_sensortower_link %></td>
          <td><%= mobile_app.android_sensortower_link %></td>
          <td><%= mobile_app.company_id %></td>
          <td><%= link_to 'Edit', edit_mobile_app_path(mobile_app) %></td>
          <td><%= link_to 'Destroy', mobile_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Mobile App', new_mobile_app_path, class: "btn btn-primary" %>
</div>
</section>